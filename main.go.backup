		return fmt.Sprintf(`<a href="#" onclick="gotoAnchor('%s')" target="%s">%s</a>`, anc, target, text)
	} 
	// Open links in new window only when their url is absolute.
	target := "_blank"
	if u, err := url.Parse(href); err != nil {

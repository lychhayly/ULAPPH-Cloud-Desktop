			//newconfigtemplate			
			case "SET_PUBWP_LIST":
				MEDIA_ID2 := r.FormValue("MEDIA_ID")
				MEDIA_ID := str2int(MEDIA_ID2)
						
				g := TDSCNFG{
						SYS_VER: 1,
						USER: uid,
						CFG_ID: "SYSTEM_Public_WP_List_Media_ID",
						DAT_TYP: "TXT",
						NUM_VAL: MEDIA_ID,

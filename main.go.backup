			BLOB_KEY := ""
			BLOB_KEY, _, _, _, _, _, _, _, _, _, _ = getTDSMEDIABlobKey(w, r, docID)	
			
 
			//var buf bytes.Buffer
			reader := blobstore.NewReader(c, appengine.BlobKey(BLOB_KEY))

			FL_PROC_OK = true
	
	}
	
	if FL_PROC_OK == false {
		return
	}
	sendMessage(w, r, ADMMAIL, "CH_MSG_NOTIFY_EVENTS", msgDtl3, "", getMapLink(w,r,msg,reqStr,SID),SID)
	
	//log num views
	laterIncNumViewsSocial.Call(c, "", SID, "SO_INC_NUM_VIEWS")
	

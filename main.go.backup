//process quiz submit button
func getBlobTextQuiz(w http.ResponseWriter, r *http.Request, SID, DATA string) (res string, pass bool, TITLE string, err error) {
	c := appengine.NewContext(r)
 
	SPD := strings.Split(DATA,"$")
	tot := 0
	pass = false
	SPL := strings.Split(SID,"-")
	TARGET := SPL[0]
	DOC_ID := "0"
	if len(SPL) > 1 {
		DOC_ID = SPL[1]
	}
	docID := str2int(DOC_ID)
	BLOB_KEY := ""
	switch TARGET {
		case "TDSSLIDE":
			BLOB_KEY, _, _, _, _, TITLE, _, _, _, _  = getTDSSLIDEBlobKey(w, r, docID)
		case "TDSARTL":
			BLOB_KEY, _, _, _, _, TITLE, _, _, _ = getTDSARTLBlobKey(w, r, docID)
		default:
			return res, false, "", fmt.Errorf("invalid operation")
	}
			
	reader := blobstore.NewReader(c, appengine.BlobKey(BLOB_KEY))
	s := bufio.NewScanner(reader)
	for s.Scan() {
		if strings.HasPrefix(s.Text(), "#") {
 
			for i:=0; i < len(SPD); i++ {
				//is item found
				//$id:answer
				if SPD[i] != "" {
					if strings.Index(s.Text(), SPD[i]) != -1 {
						tot++
					}					
				}
 
			}
		}
	}
	if err := s.Err(); err != nil {
		return res, false, "", fmt.Errorf("error operation")
	}
	//compute
 
 
	pct := percent.PercentOf(tot, len(SPD) - 1)
	res = fmt.Sprintf("%.2f", pct)
	if pct >= 75 {
		pass = true
	}
	return res, pass, TITLE, nil
}


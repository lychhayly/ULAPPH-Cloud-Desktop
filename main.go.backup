					buffer5.WriteString(fmt.Sprintf("<a href=\"https://ulapph-public-1.appspot.com/share?SH_FUNC=all&title=%v&url=%v\" target=\"share\" title=\"Share to social networks!\"><img src=\"/img/sharethis.png\" width=40 height=40></a>", "scrape", rlink))
					if len(link) > 0 { ctr++}
				}
			}				
		} else {
				if img == "Y" || img == "y"  {
					buffer5.WriteString(fmt.Sprintf("<img src=\"%v\" width=200 height=150><br>", resolveURL(url,imgLink)))
					if len(imgLink) > 0 { ctr++}
				}
				if lnk == "Y" || lnk == "y"  {
					rlink := resolveURL(url,link)
					if strings.Index(rlink, "https://") != -1 {
						buffer5.WriteString(fmt.Sprintf("<a href=\"#\" onclick=\"openWindow('%v','Scraper'); return false\">HTTPS Link</a><br>", rlink))
					} else {
						buffer5.WriteString(fmt.Sprintf("<a href=\"%v\" target=\"view\">HTTP Link</a><br>", resolveURL(url,link)))
					}
					buffer5.WriteString(fmt.Sprintf("<a href=\"https://ulapph-public-1.appspot.com/share?SH_FUNC=all&title=%v&url=%v\" target=\"share\" title=\"Share to social networks!\"><img src=\"/img/sharethis.png\" width=40 height=40></a>", "scrape", rlink))
					if len(link) > 0 { ctr++}
				}
		}
 
		msg := buffer5.String()
		i := strings.Index(msg, "<img")
		if i != -1 {
			SCR := fmt.Sprintf("<img src=\"/img/scraper.png\" width=50 height=50 title=\"%v[%v]\"> %v", url, sel, msg)
			//when scraper
			g := new(ChanMessage)
			g.User = uid
			g.Message = SCR

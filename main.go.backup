func handleServe(w http.ResponseWriter, r *http.Request) {
	
	if FL_PROC_OK := countryChecker(w,r); FL_PROC_OK != true {return}
	//prevent access from other than ulapph pages
	checkReferrer(w,r)
	
	
		c := appengine.NewContext(r)

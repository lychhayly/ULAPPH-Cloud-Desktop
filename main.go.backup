			//secCtr := 0
			for s.Scan() {
				//fmt.Fprintf(w, "%v", s.Text())
				if len(s.Text()) > 0 {
					thisStr := fmt.Sprintf("%v", s.Text())
					if string(thisStr[0]) != "#" {

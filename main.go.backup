func getHomeStatsNums(w http.ResponseWriter, r *http.Request, TARGET, NAME, div string) (m MyBox) {
			
	c := appengine.NewContext(r)
	
	q := datastore.NewQuery("TDSSTATS").Filter("VAL_TXT = ", TARGET)

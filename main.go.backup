	recCount,_ := q.Count(c)
	stats := make([]TDSSTATS, 0, recCount)
	if _, err := q.GetAll(c, &stats); err != nil {
		checkError(w,r,err,"CHK_OQ")
		return
	  }
	
	m.Element = div
	m.Name = NAME
	for _, p := range stats{
		var g MyBoxItem
		g.Value = p.VAL_NUM
		str := strings.Replace(p.STAT_NAME, TARGET, "", -1)
		g.Name = str

			return
 
		case "DELETE":
 
			MID := r.FormValue("MID")
			//_, uid := checkSession(w,r)
			if uid != FDBKMAIL {
				SPL := strings.Split(MID, "-")
				if len(SPL) <= 0 {
					return
				} else {
					if uid != SPL[0] {
						return
					}
				}
				if uid == "" || MID == "" {
					return
				}
			}
			_ = validateAccess(w, r, "IS_VALID_USER",r.URL.String())
			key := datastore.NewKey(c, "Social", MID, 0, nil)
			datastore.Delete(c, key)
			writeHTMLHeader(w, 200)
			w.Write([]byte("ok"))
			return
			
	}
 
}
 


	}
	if s != "https:" {
		switch {
			case SYS_HTTPS == false && s != "https:":
				k, _ := url.Parse(r.URL.String())
				hs := fmt.Sprintf("https://%v/", k.Host)
				url := strings.Replace(r.URL.String(), "http://", "https://", -1)

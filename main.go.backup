	c := appengine.NewContext(r)
	q := datastore.NewQuery("TDSUSERS").Limit(1)
	//c.Errorf("[S0063]")
	users := make([]TDSUSERS, 0, 1)
	if _, err := q.GetAll(c, &users); err != nil {
		panic(err)
	}
	recCount := 0
	for _, _ = range users {

//function generate random colors
func getRandomColors() (color1, color2, color3 string) {

	myrand := randNum(1, 50)
	myrand = myrand - 1
	i := 0
	ranOk := false
	for _, v := range uwmColors {
		if i == myrand {
			SPL := strings.Split(v, ",")
			if len(SPL) > 0 {
				color1 = "#"+SPL[0]
				color2 = "#"+SPL[1]
				color3 = "#"+SPL[2]
				ranOk = true
			}
			if ranOk == false {
				myrand = randNum(1, 50)
			} else {
				break
			}
		}
		i++
	}
	
	return color1, color2, color3
}
 

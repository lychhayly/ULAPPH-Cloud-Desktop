				w.WriteHeader(200)
				//update timestamp
				timestamp := getTimestamp()
				cText := fmt.Sprintf("%s-%v", uid, timestamp)
				if err := userAccessCacheDesktop0Part1.Execute(w, cText); err != nil {
				  panic(err)

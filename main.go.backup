						buffer5.WriteString(fmt.Sprintf("<a href=\"#\" onclick=\"openWindow('%v','Scraper'); return false\">HTTPS Link</a><br>", rlink))
					} else {
						buffer5.WriteString(fmt.Sprintf("<a href=\"%v\" target=\"view\">HTTP Link</a><br>", resolveURL(url,link)))

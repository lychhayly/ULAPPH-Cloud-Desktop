//handles newly uploaded blobs 
func handleUpload(w http.ResponseWriter, r *http.Request) {

	if FL_PROC_OK := checkQuotaSystem(w, r); FL_PROC_OK != true {return}
	
        c := appengine.NewContext(r)
		
        blobs, pVals, err := blobstore.ParseUpload(r)
        if err != nil {
                serveError(c, w, err)
                return
        }
        file := blobs["file"]
        if len(file) == 0 {
                ////c.Errorf("no file uploaded")
                http.Redirect(w, r, "/", http.StatusFound)
                return
        }
 
		ICON_NAME := pVals["ICON_NAME"]
		//URL_ADD_R := pVals["URL_ADD"]
		URL_ADD_R := fmt.Sprintf("%v", pVals["URL_ADD"])
		URL_ADD := strings.Replace(URL_ADD_R, "&", "@@@", -1)

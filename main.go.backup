				}
 
				//check if personal wallpaper found
				TASK_MEMCACHER_desktopWP_loginWP := ""
				wpKey := fmt.Sprintf("TASK_MEMCACHER_desktopWP_%v_desktop0_%s", "loginWP", uid)
